    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tumulte/colorGenerator.git"
  },
  "keywords": [
    "color generation",
    "color",
    "harmony"
  ],
  "author": "Tumulte",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tumulte/colorGenerator/issues"
  },
  "homepage": "https://github.com/Tumulte/colorGenerator#readme",
  "dependencies": {
    "@picaro/colorhelper": "^1.0.0",
    "core-js": "^3.20.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "babel-jest": "^27.4.4",
    "jest": "^27.4.4",
    "vite": "^2.7.2",
    "babel-loader": "^8.2.3"
  },
  "jest": {
    "verbose": false,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
