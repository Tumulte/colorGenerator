{
  "name": "@picaro/colorgenerator",
  "version": "1.2.0",
  "description": "Generate an harmonious subset of colors from a main color and a few practical parameters ",
  "main": "./dist/colorGenerator.umd.js",
  "scripts": {
    "test": "jest --watch",
    "build": "vite build"
  },
  "files": [
